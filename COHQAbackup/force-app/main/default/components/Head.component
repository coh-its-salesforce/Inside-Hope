<apex:component >
    <apex:attribute name="title" type="string" description="Page Title"> </apex:attribute>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"/>
    <title>{!title}</title>   
    
    <apex:includeScript value="{!URLFOR($Resource.jQueryPackageReport, 'package/media/js/jquery-1.11.1.min.js')}"/>
    <apex:includeScript value="{!URLFOR($Resource.jQueryPackageReport, 'package/media/js/bootstrap.min.js')}"/>
    <apex:includeScript value="{!$Resource.responsiveMenu}"/>
    <apex:includeScript value="{!$Resource.MagnificPopUpJS}"/>
