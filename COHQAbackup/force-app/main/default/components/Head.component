    <apex:stylesheet value="{!$Resource.responsiveMenuCSS}"/>
    <apex:stylesheet value="{!URLFOR($Resource.jQueryPackageReport, 'package/media/css/bootstrap.css')}"/>
    <apex:stylesheet value="{!$Resource.COH_SIP_Common}"/>
    <apex:stylesheet value="{!$Resource.MegaMenu}"/> 
    <apex:stylesheet value="{!$Resource.cohHomeCSS}"/>
    <apex:stylesheet value="{!$Resource.MagnificPopUpCSS}"/>
    <apex:stylesheet value="{!$Resource.COH_SIP_Department_Style}"/>
    
    <apex:stylesheet value="{!URLFOR($Resource.basestyles,'css/chosen.css')}"/>
