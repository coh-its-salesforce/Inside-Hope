<apex:page standardController="Contact" extensions="COH_Physician_ScholarsController" sidebar="false">
    <style>
        .alignTop {
        vertical-align: top;
        }
        .headerSearchContainer
        {
        display: none;
        }
        .pbTitle
        {
        display: none;
        }
        .activeTab {background-color: #1797C0; color:white; background-image:none}
        
        .inactiveTab { background-color: lightgrey; color:black; background-image:none}
        
    </style>
    
    <apex:form >
        <apex:pageBlock mode="edit" >
            
            <!--<div style="width:100%;">--> 
            <center> <apex:pageBlockSection columns="1" title="APPLICATION FORM B">
                <!--<br/>
<center>THE VISITING INTERNATIONAL SCHOLARS (VIS) PROGRAM</center>
<br/>
<center>DEPARTMENT OF SURGERY - CITY OF HOPE</center>--> 
                </apex:pageBlockSection></center>
          <div style='margin: 0 auto; width: 360px;'> <apex:outputLabel >Application Status: &nbsp;&nbsp;&nbsp;&nbsp;</apex:outputLabel>
                <apex:outputField value="{!con.Application_Status__c}"/></div><br/>
            
        </apex:pageBlock>
        <apex:panelGrid columns="2" columnClasses="alignTop,alignTop" cellspacing="10">
            <apex:pageBlock >
                <apex:pageBlockSection columns="1" >
                    <apex:outputtext value="{!Department}"/>
                    <apex:image value="{!Photolink}"/>
                    <apex:inputFile value="{!blobValue}" contentType="{!contentType}" fileName="{!filename}"/>
                    <div class="sidebarHeading">
                        <b><span class="headinglabel">Contact</span></b>
                        
                        <!--<apex:outputPanel rendered="{$userDetails.Contactid!}">
<a style="float:right;" data-toggle="modal" href="#usereditModal">
<span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
</a>   
</apex:outputPanel>-->
                        
                        
                        
                        
                        <hr class="saperater"/>
                        <apex:outputtext value="{!con.Name}"/> 
                        <apex:outputtext value="{!con.email}"/>
                    </div>
                </apex:pageBlockSection>
                
            </apex:pageBlock>
            <apex:tabPanel switchType="client" selectedTab="name1" id="AccountTabPanel" tabClass="activeTab" inactiveTabClass="inactiveTab">
                <apex:tab label="Basic Information" name="name1" id="tabOne">
                    
                    <apex:pageBlock >
                        
                        <apex:outputLabel >Please select a Category:&nbsp; &nbsp;  <apex:inputField value="{!con.Select_Category__c}"/> </apex:outputlabel><br/><br/>
                        <apex:pageBlockSection columns="2" title="Personal Information" >
                            
                            <apex:inputField value="{!con.FirstName}"/>
                            <apex:inputField value="{!con.LastName}"/>
                            <apex:inputField value="{!con.Nickname__c}" label="Nickname"/>
                            <apex:inputField value="{!con.Legal_Name__c}" label="Legal Name Matching Passport"/>
                            <apex:inputField value="{!con.Title}" html-placeholder="MD, PhD"/> 
                            <apex:inputField value="{!con.Position__c}" html-placeholder="Assistant Professor, Private practice, etc" label="Current Position"/> 
                            <apex:inputField value="{!con.Institution__c}" label="Current Institution"/>
                            
                            
                            <apex:inputField value="{!con.Birthdate}" />
                            <apex:inputField value="{!con.Gender__c}"/>
                            <apex:inputField value="{!con.Nationality__c}"/>
                            <apex:inputField value="{!con.Country_of_Residence__c}" label="Country of Residence Prior to coming to the US "/>
                            
                            
                        </apex:pageBlockSection> 
                        
                        <apex:pageBlockSection columns="2" title="Home Mailing Address " >
                            <apex:inputField value="{!con.MailingStreet}" label="Street"/>
                            <apex:inputField value="{!con.MailingCity}" label="City"/>
                            <apex:inputField value="{!con.MailingState}" label="State"/>
                            <apex:inputField value="{!con.MailingPostalCode}" label="PostalCode"/>
                            <apex:inputField value="{!con.MailingCountry}" label="Country"/>
                            <apex:inputField value="{!con.Home_Phone__c}" label="Phone Number"/>
                        </apex:pageBlockSection>
                        
                        <apex:pageBlockSection columns="2" title="Work Mailing Address" >
                            <apex:inputField value="{!con.street__c}"/>
                            <apex:inputField value="{!con.City__c}"/>
                            <apex:inputField value="{!con.State__c}"/>
                            <apex:inputField value="{!con.Postal_Code__c}"/>
                            <apex:inputField value="{!con.Country__c}"/>
                            <apex:inputField value="{!con.Work_Phone_Number__c}" label="Phone Number"/>
                        </apex:pageBlockSection>
                        
                        <apex:pageBlockSection columns="2" title="Contact Information" >
                            <apex:inputField value="{!con.Email}" label="Email Address"/>
                            <apex:inputField value="{!con.Phone}" label="Primary Phone Number(Cell, Work, Home)"/>
                            <apex:inputField value="{!con.OtherPhone}" label="Secondary Phone Number(Cell, Work, Home)"/>
                            
                        </apex:pageBlockSection>
                        
                        
                        
                        <apex:pageBlockSection title="Emergency Contact information" columns="2" >
                            <apex:inputField value="{!con.Emergency_Name__c}" label="Emergency Contact Name"/> 
                            <apex:inputField value="{!con.Emergency_Phone__c}" label="Emergency Contact Phone"/> 
                            <apex:inputField value="{!con.Emergency_Email__c}" label="Emergency Contact Email"/> 
                            <apex:inputField value="{!con.Relationship__c}" label="Emergency Contact Realtionship"/> 
                        </apex:pageBlockSection>
                        
                        <apex:pageBlockSection columns="1" title="Proposed Dates of the Visit">
                            Provide two time periods between one and three months in duration
                            Reminder: the application process and preparation for your visit will take at least four months from the date of approval. No dates earlier than four months after submission of your complete application will be considered
                            <apex:outputLabel >CHOICE #1: From &nbsp; &nbsp;
                                <apex:outputpanel >
                                    <apex:inputField value="{!con.Choice1StartDate__c}"/> 
                                    &nbsp;&nbsp;To&nbsp;&nbsp;
                                    <apex:inputField value="{!con.Choice1EndDate__c}"/>
                                </apex:outputpanel>
                            </apex:outputLabel>
                            <apex:outputLabel >CHOICE #2: From &nbsp; &nbsp;
                                <apex:outputpanel >
                                    <apex:inputField value="{!con.Choice2StartDate__c}"/> 
                                    &nbsp;&nbsp;To&nbsp;&nbsp;
                                    <apex:inputField value="{!con.Choice2EndDate__c}"/>
                                </apex:outputpanel>
                            </apex:outputLabel>
                        </apex:pageBlockSection>
                        
                        <apex:pageBlockSection title="Letter of Interest" columns="1" >
                            <apex:inputField value="{!con.Faculty_Sponsor__c}"/>
                            A short 300-500 words personal statement explaining why you would like to participate in our program, the goals, expectations, and clinical interests (this will help us ensure that we are able to provide you with the best experience during your visit as possible)<br/><br/>
                            <apex:inputFile value="{!attachment10.body}" fileName="{!attachment10.name}"/>
                        </apex:pageBlockSection>
                        
                        <apex:pageBlockSection columns="1" title="Required Documents" collapsible="false">
                            
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel value="1. Curriculum Vitae" for="att1"/>
                                <apex:inputFile value="{!attachment1.body}" fileName="{!attachment1.name}" id="att1"/>
                            </apex:pageBlockSectionItem><br/>
                            
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel value="2. Professional letter of recommendation" for="att2"/>
                                <apex:inputFile value="{!attachment2.body}" fileName="{!attachment2.name}" id="att12"/>
                            </apex:pageBlockSectionItem><br/>
                            
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel value="3. Current color photograph of self" for="att3"/>
                                <apex:inputFile value="{!attachment3.body}" fileName="{!attachment3.name}" id="att3"/>
                            </apex:pageBlockSectionItem><br/>
                            
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel value="4. Proof of funding (or expected funding)" for="att4"/>
                                <apex:inputFile value="{!attachment4.body}" fileName="{!attachment4.name}" id="att4"/>
                            </apex:pageBlockSectionItem><br/>
                            
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel value="5. Proof of M.D. or Ph.D. or equivalent degree (copy of your medical diploma)" for="att5"/>
                                <apex:inputFile value="{!attachment5.body}" fileName="{!attachment5.name}" id="att5"/>
                            </apex:pageBlockSectionItem><br/>
                            
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel value="6. Proof of employment" for="att6"/>
                                <apex:inputFile value="{!attachment6.body}" fileName="{!attachment6.name}" id="att6"/>
                            </apex:pageBlockSectionItem><br/>
                            
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel value="7. Proof of health insurance (Coverage during your visit to the US)" for="att7"/>
                                <apex:inputFile value="{!attachment7.body}" fileName="{!attachment7.name}" id="att7"/>
                            </apex:pageBlockSectionItem><br/>
                            <apex:pageBlockSectionItem helpText="{!$Label.Helptext}">
                                <!-- <apex:pageBlockSectionItem helpText="As a Program Sponsor,">-->
                                <apex:outputLabel value="8. Proof of English proficiency" for="at87"/>
                                <apex:inputFile value="{!attachment8.body}" fileName="{!attachment8.name}" id="att8"/>
                            </apex:pageBlockSectionItem><br/>
                            
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel value="9. Expected date of visa approval" for="att9"/>
                                <apex:inputFile value="{!attachment9.body}" fileName="{!attachment9.name}" id="att9"/>
                            </apex:pageBlockSectionItem><br/>
                            
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel value="10. Copy of Passport" for="att20"/>
                                <apex:inputFile value="{!attachment16.body}" fileName="{!attachment16.name}" id="att20"/>
                            </apex:pageBlockSectionItem><br/>
                            
                            
                        </apex:pageBlockSection>
                        <!--</div>-->
                        <apex:pageblockTable value="{!allFileList }" var="atch">
                            
                            <apex:column headerValue="View">   
                                <apex:outputLink value="{!URLFOR($Action.Attachment.Download,atch.id)}" target="_blank">Download</apex:outputLink>
                            </apex:column>    
                            <apex:column headerValue="Attachment Name">
                                <apex:outputField value="{!atch.name}" id="fileName"/>
                            </apex:column> 
                            <apex:column headerValue="Last Modified Date">   
                                <apex:outputField value="{!atch.LastModifiedDate}" id="LastModifiedDate"/>
                            </apex:column> 
                            <apex:column headerValue="{!$ObjectType.Attachment.fields.description.label}">   
                                <apex:outputField value="{!atch.description}" id="description"/>
                            </apex:column>                      
                        </apex:pageblockTable>
                        
                        <apex:pageBlockButtons >
                            <center><apex:commandButton value="Save" action="{!upload}" rendered="{!disableSave}"/> 
                                <apex:commandButton value="Submit for Approval" action="{!Approval}" rendered="{!disableSave}"/></center>
                        </apex:pageBlockButtons>
                        <!--</div>-->
                        
                    </apex:pageBlock>
                </apex:tab>
                
