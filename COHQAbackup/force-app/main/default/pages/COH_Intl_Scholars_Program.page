                                <apex:inputField value="{!visaInfo.Other_Phone_Number__c}"/>
                            </apex:pageblocksection>
                            
                        </apex:outputPanel>
                        
                        
                        
                        <apex:pageblockSection Title="Dependent 1 Visa Application Information"  >
                            <apex:inputField value="{!visaInfo.Dependent_Name1__c}" label="FirstName"/>
