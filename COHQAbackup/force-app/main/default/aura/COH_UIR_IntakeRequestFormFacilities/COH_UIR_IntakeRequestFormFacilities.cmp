<aura:component controller="COH_UIR_IntakeRequestFormController">
    <c:COH_UTIL_ValidateStandardComponent aura:id="validateStandardComponent"/>
    <c:COH_UTIL_CheckboxGroupHelper aura:id="checkboxGroupHelper"/>    
    <aura:handler name="init" value="{!this}" action="{!c.init}"/>
    <aura:method name="validateFields" action="{!c.validateFields}"/>
    <aura:attribute name="mIntakeRequest" type="COH_UniversalIntakeRequest__c"/>
    <aura:attribute name="mPicklists" type="Map" />
    <aura:attribute name="mCheckboxGroupValues" type="Map" default="{}"/>
    <aura:attribute name="mSelectedCheckboxGroupValues" type="Map" default="{}"/>
    
    <div class="slds-form-horizontal">
        <h3 class="slds-section__title slds-theme_shade slds-p-horizontal_small">
            Facilities Request
        </h3>
        
        <!-- Request Type -->
        <div class="slds-grid slds-wrap">        
            <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                <div class="slds-form-element__control">
                    <lightning:select value="{!v.mIntakeRequest.COH_UIR_FSC_FacilityRequestType__c}" 
                                      label="Select Type of Request"
                                      name="COH_UIR_FSC_FacilityRequestType__c"
                                      aura:id="intakeRequestForm"
                                      required="true">
                        <option value="">--Select--</option>
                        <aura:iteration items="{!v.mPicklists.COH_UIR_FSC_FacilityRequestType__c}" var="currentPicklistOption">
                            <option value="{!currentPicklistOption.value}" selected="{!currentPicklistOption.value==v.mIntakeRequest.COH_UIR_FSC_FacilityRequestType__c}">{!currentPicklistOption.label}</option>
                        </aura:iteration>
                    </lightning:select>
                </div>
            </div>       
        </div>
        
        <!-- If Move/New Hires -->
        <aura:if isTrue="{!v.mIntakeRequest.COH_UIR_FSC_FacilityRequestType__c == 'Move/New Hires'}">
            <div class="slds-size_1-of-1">
                <div class="slds-box slds-m-vertical_small slds-box_no-padding">            
                    <div class="slds-grid slds-wrap">
                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                            <div class="slds-form-element__control">
                                <lightning:checkboxGroup name="COH_UIR_FSC_MoveOrNewHires__c"
                                                         label="Select Type of Sub-Request"
                                                         options="{!v.mPicklists.COH_UIR_FSC_MoveOrNewHires__c}"
                                                         value="{!v.mCheckboxGroupValues.COH_UIR_FSC_MoveOrNewHires__c}"
                                                         aura:id="intakeRequestForm"
                                                         required="true"
                                                         onchange="{!c.onCheckboxGroupChange}"/>
                            </div>
                        </div>
                    </div>
                    
                    <!-- If Move Existing Employees -->
                    <aura:if isTrue="{!v.mSelectedCheckboxGroupValues.COH_UIR_FSC_MoveOrNewHires__c.MoveofExistingCOHEmployees}">
                        <div class="slds-box slds-m-around_small slds-box_x-small">
                            <div class="slds-p-horizontal_medium"><div class="slds-border_bottom"><b>Move of Existing COH Employee Details</b></div></div>
                            <!-- Number of people to be moved, Desired move date -->
                            <div class="slds-grid slds-wrap">        
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:select value="{!v.mIntakeRequest.COH_UIR_FSC_ME_NumberOfPeopleToBeMoved__c}" 
                                                          label="Enter the number of COH employees you wish to move"
                                                          name="COH_UIR_FSC_ME_NumberOfPeopleToBeMoved__c"
                                                          aura:id="intakeRequestForm"
                                                          required="true">
                                            <option value="">--Select--</option>
                                            <aura:iteration items="{!v.mPicklists.COH_UIR_FSC_ME_NumberOfPeopleToBeMoved__c}" var="currentPicklistOption">
                                                <option value="{!currentPicklistOption.value}" selected="{!currentPicklistOption.value==v.mIntakeRequest.COH_UIR_FSC_ME_NumberOfPeopleToBeMoved__c}">{!currentPicklistOption.label}</option>
                                            </aura:iteration>
                                        </lightning:select>
                                    </div>
                                </div>        
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_DesiredMoveDate__c}"
                                                         label="Select your preferred move date"
                                                         name="COH_UIR_FSC_ME_DesiredMoveDate__c"
                                                         aura:id="intakeRequestForm"
                                                         required="true"
                                                         type="date"/>
                                    </div>
                                </div>            
                            </div>        
                            
                            <!-- Desired/ Current Locations -->
                            <aura:if isTrue="{!v.mIntakeRequest.COH_UIR_FSC_ME_NumberOfPeopleToBeMoved__c >= 1}">
                                <div class="slds-grid slds-wrap">        
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_BuildingNumber1}"
                                                             label="1. Enter Desired Building (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_BuildingNumber1"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_FloorNumber1}"
                                                             label="1. Enter Desired Floor Number (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_FloorNumber1"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>                                 
                                </div>
                                <div class="slds-grid slds-wrap">        
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_ExitBuildingNumber1}"
                                                             label="1. Enter Current Building (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_ExitBuildingNumber1"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_ExitFloorNumber1}"
                                                             label="1. Enter Current Floor Number (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_ExitFloorNumber1"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>                                        
                                        </div>
                                    </div>                                 
                                </div>
                            </aura:if>
                            <aura:if isTrue="{!v.mIntakeRequest.COH_UIR_FSC_ME_NumberOfPeopleToBeMoved__c >= 2}">
                                <div class="slds-grid slds-wrap">        
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_BuildingNumber2}"
                                                             label="2. Enter Desired Building (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_BuildingNumber2"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_FloorNumber2}"
                                                             label="2. Enter Desired Floor Number (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_FloorNumber2"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>                                 
                                </div>
                                <div class="slds-grid slds-wrap">        
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_ExitBuildingNumber2}"
                                                             label="2. Enter Current Building (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_ExitBuildingNumber2"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_ExitFloorNumber2}"
                                                             label="2. Enter Current Floor Number (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_ExitFloorNumber2"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>                                 
                                </div>
                            </aura:if>
                            <aura:if isTrue="{!v.mIntakeRequest.COH_UIR_FSC_ME_NumberOfPeopleToBeMoved__c >= 3}">
                                <div class="slds-grid slds-wrap">        
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_BuildingNumber3}"
                                                             label="3. Enter Desired Building (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_BuildingNumber3"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_FloorNumber3}"
                                                             label="3. Enter Desired Floor Number (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_FloorNumber3"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>                                 
                                </div>
                                <div class="slds-grid slds-wrap">        
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_ExitBuildingNumber3}"
                                                             label="3. Enter Current Building (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_ExitBuildingNumber3"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_ExitFloorNumber3}"
                                                             label="3. Enter Current Floor Number (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_ExitFloorNumber3"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>                                 
                                </div>
                            </aura:if>
                            <aura:if isTrue="{!v.mIntakeRequest.COH_UIR_FSC_ME_NumberOfPeopleToBeMoved__c >= 4}">
                                <div class="slds-grid slds-wrap">        
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_BuildingNumber4}"
                                                             label="4. Enter Desired Building (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_BuildingNumber4"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_FloorNumber4}"
                                                             label="4. Enter Desired Floor Number (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_FloorNumber4"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>                                 
                                </div>
                                <div class="slds-grid slds-wrap">        
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_ExitBuildingNumber4}"
                                                             label="4. Enter Current Building (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_ExitBuildingNumber4"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_ExitFloorNumber4}"
                                                             label="4. Enter Current Floor Number (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_ExitFloorNumber4"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>                                 
                                </div>
                            </aura:if>
                            <aura:if isTrue="{!v.mIntakeRequest.COH_UIR_FSC_ME_NumberOfPeopleToBeMoved__c >= 5}">
                                <div class="slds-grid slds-wrap">        
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_BuildingNumber5}"
                                                             label="5. Enter Desired Building (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_BuildingNumber5"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_FloorNumber5}"
                                                             label="5. Enter Desired Floor Number (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_FloorNumber5"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>                                 
                                </div>
                                <div class="slds-grid slds-wrap">        
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_ExitBuildingNumber5}"
                                                             label="5. Enter Current Building (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_ExitBuildingNumber5"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ME_ExitFloorNumber5}"
                                                             label="5. Enter Current Floor Number (Please do not enter N/A)"
                                                             name="COH_UIR_FSC_ME_ExitFloorNumber5"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>                                 
                                </div>                                                
                            </aura:if>
                            
                            
                            <!-- Desired Location Assigned, Space Available -->
                            <div class="slds-grid slds-wrap">
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:select value="{!v.mIntakeRequest.COH_UIR_FSC_ME_DesiredLocInDepartment__c}" 
                                                          label="Is the desired location already assigned to your department?"
                                                          name="COH_UIR_FSC_ME_DesiredLocInDepartment__c"
                                                          aura:id="intakeRequestForm"
                                                          required="true">
                                            <option value="">--Select--</option>
                                            <aura:iteration items="{!v.mPicklists.COH_UIR_FSC_ME_DesiredLocInDepartment__c}" var="currentPicklistOption">
                                                <option value="{!currentPicklistOption.value}" selected="{!currentPicklistOption.value==v.mIntakeRequest.COH_UIR_FSC_ME_DesiredLocInDepartment__c}">{!currentPicklistOption.label}</option>
                                            </aura:iteration>
                                        </lightning:select>
                                    </div>
                                </div>
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:select value="{!v.mIntakeRequest.COH_UIR_FSC_ME_AvailableSpace__c}" 
                                                          label="Is there available space in your desired location?"
                                                          name="COH_UIR_FSC_ME_AvailableSpace__c"
                                                          aura:id="intakeRequestForm"
                                                          required="true">
                                            <option value="">--Select--</option>
                                            <aura:iteration items="{!v.mPicklists.COH_UIR_FSC_ME_AvailableSpace__c}" var="currentPicklistOption">
                                                <option value="{!currentPicklistOption.value}" selected="{!currentPicklistOption.value==v.mIntakeRequest.COH_UIR_FSC_ME_AvailableSpace__c}">{!currentPicklistOption.label}</option>
                                            </aura:iteration>
                                        </lightning:select>
                                    </div>
                                </div>    
                            </div>        
                            
                            <!-- Require Reconfiguration -->
                            <div class="slds-grid slds-wrap">        
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:select value="{!v.mIntakeRequest.COH_UIR_FSC_ME_ReconfigRequiredMove__c}" 
                                                          label="Does desired new location require reconfiguration?"
                                                          name="COH_UIR_FSC_ME_ReconfigRequiredMove__c"
                                                          aura:id="intakeRequestForm"
                                                          required="true">
                                            <option value="">--Select--</option>
                                            <aura:iteration items="{!v.mPicklists.COH_UIR_FSC_ME_ReconfigRequiredMove__c}" var="currentPicklistOption">
                                                <option value="{!currentPicklistOption.value}" selected="{!currentPicklistOption.value==v.mIntakeRequest.COH_UIR_FSC_ME_ReconfigRequiredMove__c}">{!currentPicklistOption.label}</option>
                                            </aura:iteration>
                                        </lightning:select>
                                    </div>
                                </div>
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:select value="{!v.mIntakeRequest.COH_UIR_FSC_ME_ReconfigRequiredExit__c}" 
                                                          label="Does exit location require reconfiguration?"
                                                          name="COH_UIR_FSC_ME_ReconfigRequiredExit__c"
                                                          aura:id="intakeRequestForm"
                                                          required="true">
                                            <option value="">--Select--</option>
                                            <aura:iteration items="{!v.mPicklists.COH_UIR_FSC_ME_ReconfigRequiredExit__c}" var="currentPicklistOption">
                                                <option value="{!currentPicklistOption.value}" selected="{!currentPicklistOption.value==v.mIntakeRequest.COH_UIR_FSC_ME_ReconfigRequiredExit__c}">{!currentPicklistOption.label}</option>
                                            </aura:iteration>
                                        </lightning:select>
                                    </div>
                                </div>                                   
                            </div>                                 
                            
                            <!-- Move Requirements -->
                            <div class="slds-grid slds-wrap">
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:checkboxGroup name="COH_UIR_FSC_ME_MoveRequirements__c"
                                                                 label="Will this move require any of the following?"
                                                                 options="{!v.mPicklists.COH_UIR_FSC_ME_MoveRequirements__c}"
                                                                 value="{!v.mCheckboxGroupValues.COH_UIR_FSC_ME_MoveRequirements__c}"
                                                                 aura:id="intakeRequestForm"
                                                                 required="true"
                                                                 onchange="{!c.onCheckboxGroupChange}"/>
                                    </div>
                                </div>
                                
                            </div>
                        </div>
                    </aura:if>
                    <!-- End Move Secion -->
                    
                    <!-- New Hire Section -->
                    <aura:if isTrue="{!v.mSelectedCheckboxGroupValues.COH_UIR_FSC_MoveOrNewHires__c.LocationforNewHire}">
                        <div class="slds-box slds-m-around_small slds-box_x-small">
                            <div class="slds-p-horizontal_medium"><div class="slds-border_bottom"><b>Location for New Hire Details</b></div></div>                        
                            <!-- # of employees that need space -->
                            <div class="slds-grid slds-wrap">        
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:select value="{!v.mIntakeRequest.COH_UIR_FSC_NH_NumberOfNewHires__c}" 
                                                          label="Enter the number of new hires which require space"
                                                          name="COH_UIR_FSC_NH_NumberOfNewHires__c"
                                                          aura:id="intakeRequestForm"
                                                          required="true">
                                            <option value="">--Select--</option>
                                            <aura:iteration items="{!v.mPicklists.COH_UIR_FSC_NH_NumberOfNewHires__c}" var="currentPicklistOption">
                                                <option value="{!currentPicklistOption.value}" selected="{!currentPicklistOption.value==v.mIntakeRequest.COH_UIR_FSC_NH_NumberOfNewHires__c}">{!currentPicklistOption.label}</option>
                                            </aura:iteration>
                                        </lightning:select>
                                    </div>
