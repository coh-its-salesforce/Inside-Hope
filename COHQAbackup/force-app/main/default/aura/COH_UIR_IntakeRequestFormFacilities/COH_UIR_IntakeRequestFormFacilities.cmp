                                </div>
                                <!-- If Funded -->
                                <aura:if isTrue="{!v.mIntakeRequest.COH_UIR_FSC_ProjectFunded__c == 'Yes'}">
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_CostCenter__c}"
                                                             label="Please enter the cost center information"
                                                             name="COH_UIR_FSC_CostCenter__c"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>
                                </aura:if>
                            </div>
                            
                            <!-- Programmatic needs been Identified -->
                            <div class="slds-grid slds-wrap">        
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:select value="{!v.mIntakeRequest.COH_UIR_FSC_ProgramNeedsNewConst__c}" 
                                                          label="Have the programmatic needs been identified?"
                                                          name="COH_UIR_FSC_ProgramNeedsNewConst__c"
                                                          aura:id="intakeRequestForm"
                                                          required="true">
                                            <option value="">--Select--</option>
                                            <aura:iteration items="{!v.mPicklists.COH_UIR_FSC_ProgramNeedsNewConst__c}" var="currentPicklistOption">
                                                <option value="{!currentPicklistOption.value}" selected="{!currentPicklistOption.value==v.mIntakeRequest.COH_UIR_FSC_ProgramNeedsNewConst__c}">{!currentPicklistOption.label}</option>
                                            </aura:iteration>
                                        </lightning:select>
                                    </div>
                                </div>
                            </div>                  
                            
                            <!-- If Programmatic needs identified -->
                            <aura:if isTrue="{!v.mIntakeRequest.COH_UIR_FSC_ProgramNeedsNewConst__c == 'Yes'}">
                                <div class="slds-grid slds-wrap">        
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:checkboxGroup name="COH_UIR_FSC_ProgramItems__c"
                                                                     label="Select program items and quantities"
                                                                     options="{!v.mPicklists.COH_UIR_FSC_ProgramItems__c}"
                                                                     value="{!v.mCheckboxGroupValues.COH_UIR_FSC_ProgramItems__c}"
                                                                     aura:id="intakeRequestForm"
                                                                     required="true"
                                                                     onchange="{!c.onCheckboxGroupChange}"/>
                                        </div>
                                    </div>
                                    <!-- If other -->
                                    <aura:if isTrue="{!v.mSelectedCheckboxGroupValues.COH_UIR_FSC_ProgramItems__c.Other}">        
                                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                            <div class="slds-form-element__control">
                                                <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ProgramItemsOther__c}"
                                                                 label="If 'Other', please specify"
                                                                 name="COH_UIR_FSC_ProgramItemsOther__c"
                                                                 aura:id="intakeRequestForm"
                                                                 required="true"/>
                                            </div>
                                        </div>
                                    </aura:if>
                                </div>
                                <div class="slds-grid slds-wrap">   
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:textarea value="{!v.mIntakeRequest.COH_UIR_FSC_ProgramQuantitiesNewConst__c}"
                                                                label="Enter the number of program quantities"
                                                                name="COH_UIR_FSC_ProgramQuantitiesNewConst__c"
                                                                aura:id="intakeRequestForm"
                                                                required="true"/>
                                        </div>         
                                    </div>   
                                </div>                    
                            </aura:if>
                        </div>
                    </aura:if>
                    
                    <!-- If Reconfiguration -->
                    <aura:if isTrue="{!v.mSelectedCheckboxGroupValues.COH_UIR_FSC_SubRequestSpacePlanning__c.Reconfiguration}">
                        <div class="slds-box slds-m-around_small slds-box_x-small">
                            <div class="slds-p-horizontal_medium"><div class="slds-border_bottom"><b>Reconfiguration Details</b></div></div>                        
                            <div class="slds-grid slds-wrap">        
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_CompletionDateReconfig__c}"
                                                         label="Desired Time of Completion"
                                                         name="COH_UIR_FSC_CompletionDateReconfig__c"
                                                         aura:id="intakeRequestForm"
                                                         required="true"
                                                         type="date"/>
                                    </div>
                                </div>
                            </div>
                            
                            <div class="slds-grid slds-wrap">        
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:checkboxGroup name="COH_UIR_FSC_ReconfigGoals__c"
                                                                 label="What is the goal of the reconfiguration?"
                                                                 options="{!v.mPicklists.COH_UIR_FSC_ReconfigGoals__c}"
                                                                 value="{!v.mCheckboxGroupValues.COH_UIR_FSC_ReconfigGoals__c}"
                                                                 aura:id="intakeRequestForm"
                                                                 required="true"
                                                                 onchange="{!c.onCheckboxGroupChange}"/>
                                    </div>
                                </div>
                                <!-- If Other -->
                                <aura:if isTrue="{!v.mSelectedCheckboxGroupValues.COH_UIR_FSC_ReconfigGoals__c.Other}">
                                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                        <div class="slds-form-element__control">
                                            <lightning:input value="{!v.mIntakeRequest.COH_UIR_FSC_ReconfigGoalsOther__c}"
                                                             label="If 'Other', please specify"
                                                             name="COH_UIR_FSC_ReconfigGoalsOther__c"
                                                             aura:id="intakeRequestForm"
                                                             required="true"/>
                                        </div>
                                    </div>        
                                </aura:if>
                            </div>                
                            
                            <div class="slds-grid slds-wrap">        
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:select value="{!v.mIntakeRequest.COH_UIR_FSC_ProgramNeedsReconfig__c}" 
                                                          label="Have the programmatic needs been identified?"
                                                          name="COH_UIR_FSC_ProgramNeedsReconfig__c"
                                                          aura:id="intakeRequestForm"
                                                          required="true">
                                            <option value="">--Select--</option>
                                            <aura:iteration items="{!v.mPicklists.COH_UIR_FSC_ProgramNeedsReconfig__c}" var="currentPicklistOption">
                                                <option value="{!currentPicklistOption.value}" selected="{!currentPicklistOption.value==v.mIntakeRequest.COH_UIR_FSC_ProgramNeedsReconfig__c}">{!currentPicklistOption.label}</option>
                                            </aura:iteration>
                                        </lightning:select>
                                    </div>
                                </div>
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:textarea value="{!v.mIntakeRequest.COH_UIR_FSC_ProgramQuantitiesReconfig__c}"
                                                            label="Select program items and quantities"
                                                            name="COH_UIR_FSC_ProgramQuantitiesReconfig__c"
                                                            aura:id="intakeRequestForm"
                                                            required="true"/>
                                    </div>         
                                </div>    
                            </div>
                        </div>
                    </aura:if>
                </div>
            </div>
        </aura:if>
    </div>
</aura:component>