<aura:component implements="lightning:actionOverride,flexipage:availableForAllPageTypes,force:hasRecordId" access="global" controller="COH_UIR_CEAFormController">
    <c:COH_UTIL_ValidateStandardComponent aura:id="validateStandardComponent"/>
    <c:COH_UTIL_CheckboxGroupHelper aura:id="checkboxGroupHelper"/>
    <aura:handler name="init" value="{!this}" action="{!c.init}"/>
    <aura:handler event="force:refreshView" action="{!c.init}" />
    <aura:attribute name="mCEAForm" type="COH_CEAForm__c" default="{}"/>
    <aura:attribute name="mCheckboxGroupValues" type="Map" default="{}"/>
    <aura:attribute name="mPicklists" type="Map" default="{}"/>
    <aura:attribute name="mSelectedCheckboxGroupValues" type="Map" default="{}"/>
    <aura:attribute name="mRecordLoaded" type="Boolean" default="false"/>
    <aura:attribute name="mTotalUnitCost" type="Integer" default="0"/>
    <aura:attribute name="mTotalInitialCost" type="Integer" default="0"/>
    <aura:attribute name="mGrandTotalCost" type="Integer" default="0"/>
    <aura:attribute name="mTotalOperatingExpenses" type="Integer" default="0"/>
    <aura:attribute name="mButtonLabel" type="String" default="Save CEA Form"/>
    <aura:attribute name="mContactInformation" type="String" default="For assistance please contact the Service Desk x8HELP"/>
    
    <div class="slds-form-horizontal slds-theme_default slds-p-around_small slds-card" aura:id="projectInformation">
        <aura:if isTrue="{!v.mRecordLoaded}">
                <div aura:id="project_information_header">
                    <h3 class="slds-section__title slds-theme_shade slds-p-horizontal_small">
                        City of Hope Capital Expenditure Authorization (CEA) Form
                    </h3>
                </div>                
                
                <div class="slds-box slds-m-vertical_small slds-box_no-padding">
                    <div class="slds-p-horizontal_medium slds-p-top_small">
                        <div class="slds-border_bottom">                
                            <h3 class="slds-section__title">
                                Corporate Accounting Use Only 
                            </h3>
                        </div>
                    </div>                
                    <div class="slds-grid slds-wrap">
                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                            <div class="slds-form-element__control">
                                <lightning:input aura:id="intakeRequestForm"
                                                 label="CEA	Number"
                                                 name="COH_CEA_CEANumber__c"
                                                 value="{!v.mCEAForm.COH_CEA_CEANumber__c}"/>
                            </div>         
                        </div>   
                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                            <div class="slds-form-element__control">
                                <lightning:input aura:id="intakeRequestForm"
                                                 label="Project Number"
                                                 name="COH_CEA_ProjectNumber__c"
                                                 value="{!v.mCEAForm.COH_CEA_ProjectNumber__c}"/>
                            </div>         
                        </div>                   
                    </div>
                </div>
                
                <!-- Item Requested, Project Description -->
                <div class="slds-grid slds-wrap">
                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                        <div class="slds-form-element__control">
                            <lightning:textarea aura:id="intakeRequestForm"
                                                label="Item Requested"
                                                name="COH_CEA_ItemRequested__c" 
                                                value="{!v.mCEAForm.COH_CEA_ItemRequested__c}"/>
                        </div>
                    </div>
                    
                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                        <div class="slds-form-element__control">
                            <lightning:textarea aura:id="intakeRequestForm"
                                                label="Project Description (Overall Objective)"
                                                name="COH_CEA_ProjectDescription__c"
                                                value="{!v.mCEAForm.COH_CEA_ProjectDescription__c}"/>
                        </div>
                    </div>
                </div>
                
                <!-- Start/End Date -->
                <aura:if isTrue="{!(v.mSelectedCheckboxGroupValues.COH_UIR_SHR_ProjectEquipmentOPEXLabor__c.Project == 'true' &amp;&amp; v.mCEAForm.COH_UIR_SHR_MultiYearProject__c == 'No') || (v.mSelectedCheckboxGroupValues.COH_UIR_SHR_ProjectEquipmentOPEXLabor__c.Equipment == 'true' &amp;&amp; v.mSelectedCheckboxGroupValues.COH_UIR_SHR_ProjectEquipmentOPEXLabor__c.Project != 'true')}">
                    <div class="slds-grid slds-wrap">        
                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                            <div class="slds-form-element__control">
                                <lightning:input aura:id="intakeRequestForm"
                                                 disabled="true"
                                                 label="Start Date"
                                                 name="COH_UIR_SHR_EstimatedStartDate__c"
                                                 value="{!v.mCEAForm.COH_UIR_SHR_EstimatedStartDate__c}"/>                
                            </div>         
                        </div>
                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                            <div class="slds-form-element__control">
                                <lightning:input aura:id="intakeRequestForm"
                                                 disabled="true"
                                                 label="End Date"
                                                 name="COH_UIR_SHR_EstimatedCompletionDate__c"
                                                 value="{!v.mCEAForm.COH_UIR_SHR_EstimatedCompletionDate__c}"/>
                            </div>         
                        </div>
                    </div>
                </aura:if>
                
                <!-- Start/End Year -->
                <aura:if isTrue="{!v.mCEAForm.COH_UIR_SHR_MultiYearProject__c == 'Yes'}">
                    <div class="slds-grid slds-wrap">    
                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                            <div class="slds-form-element__control">
                                <lightning:input aura:id="intakeRequestForm"
                                                 disabled="true"
                                                 label="Start Year"
                                                 name="COH_UIR_SHR_StartYear__c"
                                                 value="{!v.mCEAForm.COH_UIR_SHR_StartYear__c}"/>
                            </div>
                        </div>
                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                            <div class="slds-form-element__control">
                                <lightning:input aura:id="intakeRequestForm"
                                                 disabled="true"
                                                 label="End Year"
                                                 name="COH_UIR_SHR_EndYear__c"
                                                 value="{!v.mCEAForm.COH_UIR_SHR_EndYear__c}"/>                                
                            </div>
                        </div>
                    </div>
                </aura:if>
                
                <!-- Reviewed By Legal, Coordinated with ITS -->
                <div class="slds-grid slds-wrap">
                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                        <div class="slds-form-element__control">
                            <lightning:select aura:id="intakeRequestForm"
                                              label="Has this request been reviewed by Legal/Finance?" 
                                              name="COH_CEA_ReviewedByLegalFinance__c" 
                                              value="{!v.mCEAForm.COH_CEA_ReviewedByLegalFinance__c}">
                                <option value="">--Select--</option>
                                <aura:iteration items="{!v.mPicklists.COH_CEA_ReviewedByLegalFinance__c}" var="currentPicklistOption">
                                    <option value="{#currentPicklistOption.value}" >{#currentPicklistOption.label}</option>
                                </aura:iteration>
                            </lightning:select>                                
                        </div>
                    </div>
                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                        <div class="slds-form-element__control">
                            <lightning:select aura:id="intakeRequestForm"
                                              label="Has this request been coordinated with ITS/Facilities?" 
                                              name="COH_CEA_CoordinatedWithITSFacilities__c" 
                                              value="{!v.mCEAForm.COH_CEA_CoordinatedWithITSFacilities__c}">
                                <option value="">--Select--</option>
                                <aura:iteration items="{!v.mPicklists.COH_CEA_CoordinatedWithITSFacilities__c}" var="currentPicklistOption">
                                    <option value="{#currentPicklistOption.value}" >{#currentPicklistOption.label}</option>
                                </aura:iteration>
                            </lightning:select>                                
                        </div>
                    </div>                
                </div>
                
                <!-- Asset Classification -->
                <div class="slds-grid slds-wrap">
                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                        <div class="slds-form-element__control">
                            <lightning:select aura:id="intakeRequestForm"
                                              label="Asset Classification" 
                                              name="COH_CEA_AssetClassification__c" 
                                              value="{!v.mCEAForm.COH_CEA_AssetClassification__c}">
                                <option value="">--Select--</option>
                                <aura:iteration items="{!v.mPicklists.COH_CEA_AssetClassification__c}" var="currentPicklistOption">
                                    <option value="{#currentPicklistOption.value}" >{#currentPicklistOption.label}</option>
                                </aura:iteration>
                            </lightning:select>                                
                        </div>
                    </div>
                </div>
                
                <!-- Building Number, Room Number -->
                <div class="slds-grid slds-wrap">
                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                        <div class="slds-form-element__control">
                            <lightning:input aura:id="intakeRequestForm"
                                             label="Equipment Building Number (Required)"
                                             name="COH_CEA_EquipmentBuildingNumber__c"
                                             value="{!v.mCEAForm.COH_CEA_EquipmentBuildingNumber__c}"/>
                        </div>         
                    </div>   
                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                        <div class="slds-form-element__control">
                            <lightning:input aura:id="intakeRequestForm"
                                             label="Equipment Room Number (Required)"
                                             name="COH_CEA_EquipmentRoomNumber__c"
                                             value="{!v.mCEAForm.COH_CEA_EquipmentRoomNumber__c}"/>
                        </div>         
                    </div>  
                </div>
                
                <!-- Entity Number -->
                <div class="slds-grid slds-wrap">
                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                        <div class="slds-form-element__control">
                            <lightning:input aura:id="intakeRequestForm"
                                             label="Requesting Entity"
                                             name="COH_CEA_RequestingEntity__c"
                                             value="{!v.mCEAForm.COH_CEA_RequestingEntity__c}"/>
                        </div>         
                    </div>  
                </div>
                
                <!-- Department Name, Department Number -->
                <div class="slds-grid slds-wrap">
                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                        <div class="slds-form-element__control">
                            <lightning:input aura:id="intakeRequestForm"
                                             label="Requesting Department Name"
                                             name="COH_CEA_RequestingDepartmentName__c"
                                             value="{!v.mCEAForm.COH_CEA_RequestingDepartmentName__c}"/>
                        </div>         
                    </div>   
                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                        <div class="slds-form-element__control">
                            <lightning:input aura:id="intakeRequestForm"
                                             label="Requesting Department Number"
                                             name="COH_CEA_DepartmentNumber__c"
                                             value="{!v.mCEAForm.COH_CEA_DepartmentNumber__c}"/>
                        </div>         
                    </div>       
                </div>
                
                <aura:if isTrue="{!v.mCEAForm.COH_CEA_DepartmentNumber__c == '8610070' || v.mCEAForm.COH_CEA_DepartmentNumber__c == '8610071' || v.mCEAForm.COH_CEA_DepartmentNumber__c == '8610160'}">
                    <div class="slds-grid slds-wrap">
                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                            <div class="slds-form-element__control">
                                <lightning:input aura:id="intakeRequestForm"
                                                 label="Primary Facilities Project Manager Employee Id"
                                                 maxlength="10"
                                                 name="COH_CEA_PrimaryFacilitiesPMEmployeeId__c"
                                                 pattern="^[0-9]*"
                                                 messageWhenPatternMismatch="Employee Id can only consist of numbers"
                                                 value="{!v.mCEAForm.COH_CEA_PrimaryFacilitiesPMEmployeeId__c}"/>
                            </div>         
                        </div>   
                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                            <div class="slds-form-element__control">
                                <lightning:input aura:id="intakeRequestForm"
                                                 label="Primary Facilities Project Manager Name"
                                                 maxlength="50"
                                                 name="COH_CEA_PrimaryFacilitiesPMName__c"
                                                 value="{!v.mCEAForm.COH_CEA_PrimaryFacilitiesPMName__c}"/>
                            </div>         
                        </div>       
                    </div>
                    <div class="slds-grid slds-wrap">
                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                            <div class="slds-form-element__control">
                                <lightning:input aura:id="intakeRequestForm"
                                                 label="Backup Facilities Project Manager Employee Id"
                                                 maxlength="10"
                                                 name="COH_CEA_BackupFacilitiesPMEmployeeId__c"
                                                 pattern="^[0-9]*"
                                                 messageWhenPatternMismatch="Employee Id can only consist of numbers"
                                                 value="{!v.mCEAForm.COH_CEA_BackupFacilitiesPMEmployeeId__c}"/>
                            </div>         
                        </div>   
                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                            <div class="slds-form-element__control">
                                <lightning:input aura:id="intakeRequestForm"
                                                 label="Backup Facilities Project Manager Name"
                                                 maxlength="50"
                                                 name="COH_CEA_BackupFacilitiesPMName__c"
                                                 value="{!v.mCEAForm.COH_CEA_BackupFacilitiesPMName__c}"/>
                            </div>         
                        </div>       
                    </div>                     
                </aura:if>        
                
                <!-- Funding Type -->
                <div class="slds-size_1-of-1">
                    <div class="slds-box slds-m-vertical_small slds-box_no-padding ">
                        <div class="slds-grid slds-wrap">
                            <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">              
                                <div class="slds-form-element__control">
                                    <lightning:checkboxGroup aura:id="intakeRequestFormCheckboxGroup"
                                                             label="Funding"
                                                             name="COH_CEA_Funding__c"
                                                             onchange="{!c.onCheckboxGroupChange}"
                                                             options="{!v.mPicklists.COH_CEA_Funding__c}"
                                                             value="{!v.mCheckboxGroupValues.COH_CEA_Funding__c}"/>                                 
                                </div>         
                            </div>                    
                        </div>
                        
                        <!-- If COH_CEA_Funding Contains Grant -->
                        <!-- Grant Entity, Grant Cost Center Number -->
                        <aura:if isTrue="{!or(v.mSelectedCheckboxGroupValues.COH_CEA_Funding__c.Grant, v.mSelectedCheckboxGroupValues.COH_CEA_Funding__c.Discretionary)}">
                            <div class="slds-grid slds-wrap">
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:input aura:id="intakeRequestForm"
                                                         label="Grant Discretionary Entity"
                                                         name="COH_CEA_GrantEntity__c"
                                                         value="{!v.mCEAForm.COH_CEA_GrantEntity__c}"/>
                                    </div>         
                                </div>   
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:input aura:id="intakeRequestForm"
                                                         label="Grant Department Number"
                                                         name="COH_CEA_GrantCostCenterNumber__c"
                                                         value="{!v.mCEAForm.COH_CEA_GrantCostCenterNumber__c}"/>
                                    </div>         
                                </div>                       
                            </div>
                            
                            <!-- PCBU, Grant Project Id -->
                            <div class="slds-grid slds-wrap">
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:input aura:id="intakeRequestForm"
                                                         label="PCBU"
                                                         name="COH_CEA_PCBU__c"
                                                         value="{!v.mCEAForm.COH_CEA_PCBU__c}"/>
                                    </div>         
                                </div>   
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:input aura:id="intakeRequestForm"
                                                         label="Grant Project Id"
                                                         name="COH_CEA_GrantProjectId__c"
                                                         value="{!v.mCEAForm.COH_CEA_GrantProjectId__c}"/>
                                    </div>         
                                </div>                       
                            </div>                            
                            
                            <div class="slds-grid slds-wrap">
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:select aura:id="intakeRequestForm"
                                                          label="If Grant, what is the category?" 
                                                          name="COH_CEA_GrantCategory__c"
                                                          value="{!v.mCEAForm.COH_CEA_GrantCategory__c}">
                                            <option value="">--Select--</option>
                                            <aura:iteration items="{!v.mPicklists.COH_CEA_GrantCategory__c}" var="currentPicklistOption">
                                                <option value="{#currentPicklistOption.value}" >{#currentPicklistOption.label}</option>
                                            </aura:iteration>
                                        </lightning:select>                                
                                    </div>
                                </div>
                                <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                                    <div class="slds-form-element__control">
                                        <lightning:input aura:id="intakeRequestForm"
                                                         label="Chart String"
                                                         name="COH_CEA_GrantChartString__c"
                                                         value="{!v.mCEAForm.COH_CEA_GrantChartString__c}"/>
                                    </div>         
                                </div>
                            </div>
                        </aura:if>
                    </div>
                </div>
                
                <!-- Electronic Tracking Tag -->
                <div class="slds-grid slds-wrap">
                    <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                        <div class="slds-form-element__control">
                            <lightning:select aura:id="intakeRequestForm"
                                              label="Need Electronic Tracking Tag?"
                                              name="COH_CEA_NeedElectronicTrackingTag__c"
                                              value="{!v.mCEAForm.COH_CEA_NeedElectronicTrackingTag__c}">
                                <option value="">--Select--</option>
                                <aura:iteration items="{!v.mPicklists.COH_CEA_NeedElectronicTrackingTag__c}" var="currentPicklistOption">
                                    <option value="{#currentPicklistOption.value}" >{#currentPicklistOption.label}</option>
                                </aura:iteration>
                            </lightning:select>                                
                        </div>
                    </div>    
                </div>
                
                <!-- Capital Costs -->
                <div class="slds-box slds-m-vertical_small slds-box_no-padding">
                    <div class="slds-p-horizontal_medium slds-p-top_small">
                        <div class="slds-border_bottom">                
                            <h3 class="slds-section__title">
                                Capital Costs
                            </h3>
                        </div>
                    </div>
                    
                    <!-- Unit Cost, Quantity -->
                    <div class="slds-grid slds-wrap">
                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                            <div class="slds-form-element__control">
                                <lightning:input aura:id="intakeRequestForm"
                                                 formatter="currency"
                                                 label="Unit Cost"
                                                 min="0"
                                                 name="COH_CEA_UnitCost__c"
                                                 onchange="{!c.unitCostChange}"
                                                 step="0.01"
                                                 type="number"
                                                 value="{!v.mCEAForm.COH_CEA_UnitCost__c}"/>
                            </div>         
                        </div>
                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                            <div class="slds-form-element__control">
                                <lightning:input aura:id="intakeRequestForm"
                                                 label="Quantity"
                                                 min="0"
                                                 name="COH_CEA_Quantity__c"
                                                 onchange="{!c.unitCostChange}"
                                                 type="number"
                                                 value="{!v.mCEAForm.COH_CEA_Quantity__c}"/>
                            </div>         
                        </div>       
                    </div>
                    
                    <!-- Total Unit Cost -->
                    <div class="slds-grid slds-wrap">
                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                            <div class="slds-form-element__control">
                                <lightning:input disabled="true"
                                                 formatter="currency"
                                                 label="Total Unit Cost"
                                                 name="mTotalUnitCost"
                                                 step="0.01"
                                                 type="number"
                                                 value="{!v.mTotalUnitCost}"/>
                            </div>         
                        </div>
                    </div>
                    
                    <!-- Freight, Sales Tax -->
                    <div class="slds-grid slds-wrap">
                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                            <div class="slds-form-element__control">
                                <lightning:input aura:id="intakeRequestForm"
                                                 formatter="currency"
                                                 label="Freight"
                                                 min="0"
                                                 name="COH_CEA_Freight__c"
                                                 onchange="{!c.totalCostChange}"
                                                 step="0.01"
                                                 type="number"
                                                 value="{!v.mCEAForm.COH_CEA_Freight__c}"/>
                            </div>         
                        </div>
                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                            <div class="slds-form-element__control">
                                <lightning:input aura:id="intakeRequestForm"
                                                 formatter="currency"
                                                 label="Sales tax"
                                                 min="0"
                                                 name="COH_CEA_SalesTax__c"
                                                 onchange="{!c.totalCostChange}"
                                                 step="0.01"
                                                 type="number"
                                                 value="{!v.mCEAForm.COH_CEA_SalesTax__c}"/>
                            </div>         
                        </div>       
                    </div>
                    
                    <!-- Installation, De-Installation -->
                    <div class="slds-grid slds-wrap">
                        <div class="slds-size_1-of-1 slds-medium-size_1-of-2 slds-p-horizontal_medium slds-p-vertical_small">
                            <div class="slds-form-element__control">
                                <lightning:input aura:id="intakeRequestForm"
                                                 formatter="currency"
                                                 label="Installation, renovation, &amp; additional space"
                                                 min="0"
                                                 name="COH_CEA_Installation__c"
                                                 onchange="{!c.totalCostChange}"
                                                 step="0.01"
