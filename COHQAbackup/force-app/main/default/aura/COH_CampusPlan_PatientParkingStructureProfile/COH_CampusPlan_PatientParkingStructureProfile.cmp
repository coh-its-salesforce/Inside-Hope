<aura:component>
   <aura:attribute name="openDuarteBuildings" type="boolean" default="false"/>
   <aura:attribute name="PatientParkingStructure" type="boolean" default="true"/>
   <aura:if isTrue="{!v.PatientParkingStructure}">
    <div class="banner-container">
    	<div class="slds-grid slds-wrap">
      		<div class="slds-col slds-size_1-of-1 slds-medium-size_12-of-12 slds-large-size_7-of-12">
      			<div class="banner-image">
      				<img src = "{!$Resource.cpMedicalOfficeBuilding}"/> 
      			</div>
      		</div>
           <div class="slds-col slds-size_1-of-1 slds-medium-size_12-of-12 slds-large-size_5-of-12" >
               <div class="banner-content">
      	   			<h4 class="label-header"><label class="blue-bg">Duarte</label></h4>
                    <h2> Patient Parking Structure</h2>
                   <p class="banner-p">
                       <strong>Purpose</strong><br/>
                       A new multi-level parking structure will simplify access for our patients.                      
                   </p>
                   <p class="banner-p">
                       <strong>Capacity</strong><br/>
                       Approximately 1,000 parking spaces.                      
                   </p>
                   <p class="banner-p">
                       <strong>Location</strong><br/>
                       Northeast corner of Duarte campus in parking lot A                      
                   </p>
                   <p class="banner-p">
                       <strong>Notes</strong><br/>
                       Construction begins in summer 2020.                     
                   </p>
                   <p class="banner-p">
                       <strong>Opening</strong><br/>
                       End of 2021                     
                   </p>    		
               </div>
      	   </div>
      	</div>
       </div>
		
        <div class="backbutton" onclick="{!c.goBackToDuarteBuildingPage}" >
            <lightning:icon iconName="utility:back" size="x-small" variant="inverse" />
            &nbsp;&nbsp;Back  
   		</div>
       </aura:if>
    
        <aura:if isTrue="{!v.openDuarteBuildings}">
            
                <c:COH_CampusPlan_DuarteBuilding/>
        </aura:if>
</aura:component>