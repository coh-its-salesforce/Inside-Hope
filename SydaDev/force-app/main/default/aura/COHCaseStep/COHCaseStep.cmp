<aura:component >
    <aura:attribute name="mComplete" type="Object" default=""/>
    <aura:attribute name="mCompleteText" type="String" default=""/>
    <aura:attribute name="mIncompleteText" type="String" default=""/>
    
    <div class="slds-grid slds-wrap">
        <div class="slds-col slds-size_1-of-1 slds-p-bottom_xx-small">
            <aura:if isTrue="{!v.mComplete}">
                <lightning:icon iconName="action:approval" alternativeText="Complete" size="xx-small" />
                <!--<span class="slds-text-heading_small slds-p-horizontal_x-small">{!v.mCompleteText}</span>-->
                <aura:set attribute="else">
                    <lightning:icon iconName="action:close" alternativeText="Incomplete" size="xx-small" />
                   <!-- <span class="slds-text-heading_small slds-p-horizontal_x-small">{!v.mIncompleteText}</span>-->
                </aura:set>
            </aura:if>
        </div>
    </div>
</aura:component>